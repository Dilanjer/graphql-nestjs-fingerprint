{
  "name": "graphql-nestjs-fingerprint",
  "version": "1.0.1",
  "description": "Server-side fingerprinting library for framework NestJs & Graphql",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "author": "dilanjer",
  "repository": "Dilanjer/graphql-nestjs-fingerprint",
  "files": [
    "dist",
    "package.json",
    "README.md"
  ],
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "cd example && npm i && cd ../ && jest --config jest-test.json",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "keywords": [],
  "license": "MIT",
  "devDependencies": {
    "@nestjs/common": "^10.2.2",
    "@nestjs/core": "^10.2.2",
    "@nestjs/graphql": "^12.0.11",
    "@nestjs/testing": "^10.2.3",
    "@types/jest": "^29.5.4",
    "jest": "^29.6.4",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "tslint": "^6.1.3",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@nestjs/platform-express": "^10.2.3",
    "cookie-parser": "^1.4.6",
    "express": "^4.18.2",
    "murmurhash3js": "^3.0.1",
    "request-ip": "^3.3.0",
    "useragent": "^2.3.0"
  },
  "peerDependencies": {
    "@nestjs/common": "*",
    "@nestjs/core": "*",
    "@nestjs/graphql": "*"
  }
}